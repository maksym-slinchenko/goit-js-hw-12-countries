{{#each this}}
<h2 class="country-name">{{name}}</h2>
<div class="country-secondary-container">

    <div class="country-description-container">
        <h2>Capital: {{capital}}</h2>
        <h2>Population: {{population}}</h2>
        <h2>Languages:</h2>
        <ul data-languages-list="languages-list"></ul>
    </div>
    <img class="flag-image" src="{{flag}}" alt="{{name}}">
</div>
{{/each}}